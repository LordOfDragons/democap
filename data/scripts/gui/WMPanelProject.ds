	/** Move object. */
	public func void moveObject(DVector direction)
		if pObject != null
			var Element element = pObject.getElement()
			var DMatrix matrix = DMatrix.newFromQuaternion(element.getOrientation())
			element.setPosition(element.getPosition() + matrix.transformNormal(direction)\
				* SessionSettings.get().getManualMovementStep())
		end
	end
	
	/** Rotate object. */
	public func void rotateObject(Vector axis)
		if pObject != null
			var Element element = pObject.getElement()
			var Matrix matrix = Matrix.newFromQuaternion(element.getOrientation())
			element.setOrientation(element.getOrientation() * Quaternion.newFromAxis(\
				matrix.transformNormal(axis), -SessionSettings.get().getManualRotationStep()))
		end
	end
	
	
	
