		getClipboard().getClip().findCastable(block StubElement stub
			stub = StubElement.new(stub)
			var BehaviorElementClass eclass = stub.getClass() cast BehaviorElementClass
			var ECBSceneObject sceneObject = ECBSceneObject.getBehaviorIn(eclass)
			
			if eclass.getPersistable()
				stub.setID(GameApp.getGameApp().getIDGenerator().nextID())
			end
			
			var Camera camera = GameApp.getGameApp().getWindowGameWorld().getCamera()
			var DVector position = DVector.new()
			if camera != null
				position = camera.getMatrix() * DVector.new(0, 0, 1)
