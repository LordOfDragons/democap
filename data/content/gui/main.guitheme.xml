		<font parameter='normal.font' size='15'>/shareddata/guithemes/modern/fonts/sans15.defont</font>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
	</designer>
	
	<designer name='Panel.WMPanelConfiguration.TrackerSlots' type='Widget'>
